{
    "name": "widop/github-hook-provider",
    "description": "Github WebHook job with Silex",
    "keywords": [ "gitub", "hook", "silex" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Widop",
            "email": "contact@widop.com"
        },
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        }
    ],
    "require": {
        "silex/silex": "~1.0",
        "symfony/config": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": " ~4.0",
        "symfony/browser-kit": "~2.0",
        "monolog/monolog": "~1.0"
    },
    "suggest": {
        "monolog/monolog": "Enables debugging"
    },
    "autoload": {
        "psr-0": { "Widop\\GithubHook": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "target-dir": "Widop/GithubHook",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
